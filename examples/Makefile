toROSE:
	make diffROSE op=cp  dir="to"

local_toROSE:
	make local_diffROSE op=cp  dir="to"

fromROSE:
	make diffROSE op=cp dir=from

local_fromROSE:
	make local_diffROSE op=cp dir=from

