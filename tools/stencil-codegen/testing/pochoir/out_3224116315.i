                for (int r = 0; r < N_RANK+1; ++i) {
                    shape_[i].shift[r] = shape1[i].shift[r];
                }
            }
            for (i = 0; i < N_SIZE2; ++i) {
                if (shape2[i].shift[0] < l_min_time_shift)
                    l_min_time_shift = shape2[i].shift[0];
                if (shape2[i].shift[0] > l_max_time_shift)
