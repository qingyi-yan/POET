    operator==(const basic_string<_CharT>& __lhs,
	       const basic_string<_CharT>& __rhs) noexcept
    { return (__lhs.size() == __rhs.size()
	      && !std::char_traits<_CharT>::compare(__lhs.data(), __rhs.data(),
						    __lhs.size())); }

